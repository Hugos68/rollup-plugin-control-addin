{
	"name": "rollup-plugin-control-addin",
	"description": "A Rollup plugin for building AL Control Add-in's.",
	"version": "0.0.2",
	"license": "MIT",
	"keywords": ["rollup-plugin", "control-addin"],
	"author": {
		"name": "Hugo Korte",
		"email": "hugokorteapple@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Hugos68/rollup-plugin-control-addin"
	},
	"files": ["dist"],
	"sideEffects": false,
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		}
	},
	"scripts": {
		"build": "unbuild",
		"format": "biome check --write",
		"format:check": "biome check"
	},
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@changesets/cli": "^2.27.11",
		"rollup": "^4.29.1",
		"typescript": "^5.7.2",
		"unbuild": "^3.2.0"
	},
	"type": "module",
	"dependencies": {
		"rollup-plugin-import-css": "^3.5.8"
	}
}
